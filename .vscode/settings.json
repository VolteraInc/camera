{
    "python.pythonPath": "venv/bin/python",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/venv/**": true,
        "**/*.pyc": true,
        "**/__pycache__": true,
      },
      "restructuredtext.confPath": "${workspaceFolder}/docs/source",
      "terminal.integrated.lineHeight": 1.1,
      "files.associations": {
        "*.tcc": "cpp"
      },
      "C_Cpp.clang_format_fallbackStyle": "LLVM",
}